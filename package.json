{
  "dependencies": {
    "websocket": "1.0.32"
  },
  "devDependencies": {
    "@types/node": "14.11.5",
    "@types/websocket": "1.0.1",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.4.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -c lint-staged.config.js"
    }
  },
  "lint-staged": {
    "**/*": [
      "npm run fix"
    ]
  },
  "private": true,
  "scripts": {
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --ext .ts --fix --ignore-path .gitignore .",
    "fix:prettier": "prettier --ignore-path .gitignore --write .",
    "heroku-postbuild": "tsc",
    "start": "tsc -w"
  }
}
